:DailyModifiedHargreaves a :EstimationMethod,
        owl:NamedIndividual ;
    rdfs:label "Daily Modified Hargreaves" ;
    :hasDataSource :COAgriculturalMeteorologyNetworkStation,
        <http://purl.org/iow/WaterBudgetingFramework/CommunityCollaborativeRain%2CHailAndSnowNetwork%28CoCoRaHS%29>,
        :NationalOceanicAndAtmosphericAdministrationStations,
